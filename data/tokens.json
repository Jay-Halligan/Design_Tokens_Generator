{
  "global": {
    "fontFamilies": {
      "lato": {
        "value": "Lato",
        "type": "fontFamilies"
      },
      "roboto": {
        "value": "Roboto",
        "type": "fontFamilies"
      }
    },
    "lineHeights": {
      "0": {
        "value": "AUTO",
        "type": "lineHeights"
      },
      "auto": {
        "value": "AUTO",
        "type": "lineHeights"
      }
    },
    "fontWeights": {
      "regular": {
        "value": "Regular",
        "type": "fontWeights"
      },
      "bold": {
        "value": "Bold",
        "type": "fontWeights"
      },
      "lato-regular": {
        "value": "Regular",
        "type": "fontWeights"
      },
      "roboto-regular": {
        "value": "Regular",
        "type": "fontWeights"
      }
    },
    "fontSize": {
      "0": {
        "value": "16",
        "type": "fontSizes"
      },
      "1": {
        "value": "64",
        "type": "fontSizes"
      },
      "16": {
        "value": "16",
        "type": "fontSizes"
      },
      "18": {
        "value": "18",
        "type": "fontSizes"
      },
      "h1": {
        "value": "64",
        "type": "fontSizes"
      },
      "h6": {
        "value": "22",
        "type": "fontSizes"
      }
    },
    "letterSpacing": {
      "0": {
        "value": "-0.5",
        "type": "letterSpacing"
      },
      "1": {
        "value": "0.5",
        "type": "letterSpacing"
      }
    },
    "paragraphSpacing": {
      "0": {
        "value": "0",
        "type": "paragraphSpacing"
      }
    },
    "text": {
      "header": {
        "h1": {
          "value": {
            "fontFamily": "$fontFamilies.lato",
            "fontWeight": "{fontWeights.lato-regular}",
            "lineHeight": "$lineHeights.0",
            "fontSize": "$fontSize.1",
            "letterSpacing": "$letterSpacing.0",
            "paragraphSpacing": "$paragraphSpacing.0",
            "textCase": "$textCase.none",
            "textDecoration": "$textDecoration.none"
          },
          "type": "typography"
        },
        "h6": {
          "value": {
            "fontFamily": "$fontFamilies.lato",
            "fontWeight": "Bold",
            "lineHeight": "AUTO",
            "fontSize": "$fontSize.h6",
            "letterSpacing": "0%",
            "paragraphSpacing": "0",
            "textDecoration": "none",
            "textCase": "none"
          },
          "description": "h6",
          "type": "typography"
        },
        "h5": {
          "value": {
            "fontFamily": "{lato}",
            "fontWeight": "{bold}",
            "lineHeight": "AUTO",
            "fontSize": "24",
            "letterSpacing": "0%",
            "paragraphSpacing": "0",
            "textDecoration": "none",
            "textCase": "none"
          },
          "description": "h6",
          "type": "typography"
        }
      },
      "body": {
        "default": {
          "regular": {
            "value": {
              "fontFamily": "$fontFamilies.roboto",
              "fontWeight": "{fontWeights.roboto-regular}",
              "lineHeight": "$lineHeights.0",
              "fontSize": "$fontSize.0",
              "letterSpacing": "$letterSpacing.1",
              "paragraphSpacing": "$paragraphSpacing.0",
              "textCase": "$textCase.none",
              "textDecoration": "$textDecoration.none"
            },
            "type": "typography"
          },
          "bold": {
            "value": {
              "fontFamily": "$fontFamilies.roboto",
              "fontWeight": "$fontWeights.bold",
              "lineHeight": "{lineHeights.auto}",
              "fontSize": "{fontSize.16}",
              "letterSpacing": "$letterSpacing.1",
              "paragraphSpacing": "$paragraphSpacing.0",
              "textCase": "$textCase.none",
              "textDecoration": "$textDecoration.none"
            },
            "type": "typography"
          }
        }
      }
    },
    "textCase": {
      "none": {
        "value": "none",
        "type": "textCase"
      }
    },
    "textDecoration": {
      "none": {
        "value": "none",
        "type": "textDecoration"
      }
    }
  }
}